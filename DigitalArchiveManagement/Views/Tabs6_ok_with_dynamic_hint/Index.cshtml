<br />
<div id="tabStripWrapper" class="tabstrip-wrapper">
    <div id="tabstripTop" class="tabstrip-top-container"></div>
</div>

<script type="text/javascript">
    var cc;
    var initialTabControl = function () {
        cc = new hTabControl("tabstripTop");
        cc.IsFirstTabClosible = true;//false时第一个tab不加关闭按钮
    };

    var addTab1 = function () {
        if (cc != null)
            cc.AddTab('a', '我的MVC', "/button/index");
        //cc.AddTab('d', '百度', "www.baidu.com", true);//为true的，加载时默认首页
    }

    var addTab2 = function () {
        if (cc != null)
            cc.AddTab('b', '我的TABs1', "/tabs/index");
    }

    var addTab3 = function () {
        if (cc != null)
            cc.AddTab('c', '我的TABs2', "/tabs2/index");
    }

    var addTab4 = function () {
        if (cc != null)
            cc.AddTab('d', '腾讯首页-QQ', "www.qq.com");
    }

    var addTab5 = function () {
        if (cc != null)
            cc.AddTab('e', '百度搜索', "www.baidu.com");
    }

    var addTab6 = function () {
        if (cc != null)
            cc.AddTab('f', '新浪首页', "www.sina.com.cn");
    }

    var addTab7 = function () {
        if (cc != null)
            cc.AddTab('g', '163网易', "www.163.com");
    }

    var addTab8 = function () {
        if (cc != null)
            cc.AddTab('h', '126邮箱', "www.126.com");
    }

    var addTab9 = function () {
        if (cc != null)
            cc.AddTab('j', '搜狐首页', "www.sohu.com");
    }

    var addTab10 = function () {
        if (cc != null)
            cc.AddTab('k', '京东购物', "www.jd.com");
    }
</script>
